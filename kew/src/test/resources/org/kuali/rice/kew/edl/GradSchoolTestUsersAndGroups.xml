<?xml version="1.0" encoding="UTF-8"?>
<!--
  Groups to satisfy the GradSchoolRoutingConfiguration for Unit Testing
  user1 and user2 are defined in the DefaultTestData.xml
  (Quickstart also has a superset of these defined)
-->
<data xmlns="ns:workflow" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="ns:workflow resource:WorkflowData">
  <groups xmlns="ns:workflow/Group" xsi:schemaLocation="ns:workflow/Group resource:Group">
    <group>
      <id>1</id>
      <name>Group1</name>
      <namespace>KR-WKFLW</namespace>
      <description>Group1</description>
      <members>
        <principalName>user1</principalName>
        <principalName>user3</principalName>
      </members>
    </group>
    <group>
      <id>2</id>
      <name>Group2</name>
      <namespace>KR-WKFLW</namespace>
      <description>Group2</description>
      <members>
        <principalName>user2</principalName>
      </members>
    </group>
  </groups>
</data>
